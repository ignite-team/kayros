<?xml version="1.0" encoding="UTF-8"?>
<zk>
	<config-name>kayros-config</config-name>
	<library-property>
		<name>org.zkoss.web.preferred.dateFormatInfo</name>
		<value>es.ozona.kayros.webapp.utils.converters.DateTimeFormater</value>
	</library-property>
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>~./zul/errorHandler.zul</location>
	</error-page>
	<session-config>
		<session-timeout>600</session-timeout>
		<automatic-timeout>true</automatic-timeout>
		<device-type>ajax</device-type>
		<timeout-uri>/logout</timeout-uri>
	</session-config>
</zk>